{
  "dashboard": {
    "id": null,
    "title": "Music21 MCP Server - Enterprise Monitoring",
    "tags": ["music21", "mcp", "enterprise"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(mcp_requests_total[5m]))",
            "legendFormat": "Requests/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "min": 0
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(mcp_requests_total{status=\"error\"}[5m])) / sum(rate(mcp_requests_total[5m])) * 100",
            "legendFormat": "Error %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Active Sessions",
        "type": "stat",
        "targets": [
          {
            "expr": "mcp_active_sessions",
            "legendFormat": "Sessions"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Response Time (95th percentile)",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(mcp_request_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "P95 Latency"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "min": 0,
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Request Rate by Tool",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(mcp_requests_total[5m])) by (tool)",
            "legendFormat": "{{ tool }}"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Request Duration by Tool",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(mcp_request_duration_seconds_bucket[5m])) by (le, tool))",
            "legendFormat": "{{ tool }} P95"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(mcp_request_duration_seconds_bucket[5m])) by (le, tool))",
            "legendFormat": "{{ tool }} P50"
          }
        ],
        "yAxes": [
          {
            "label": "Duration (seconds)",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "Music Analysis Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(music21_analysis_duration_seconds_bucket[5m])) by (le, analysis_type))",
            "legendFormat": "{{ analysis_type }} P95"
          }
        ],
        "yAxes": [
          {
            "label": "Duration (seconds)",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "Analysis Queue Size",
        "type": "graph",
        "targets": [
          {
            "expr": "music21_analysis_queue_size",
            "legendFormat": "Queue Size"
          }
        ],
        "yAxes": [
          {
            "label": "Queue Depth",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 9,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "music21_memory_usage_bytes{type=\"rss\"} / 1024 / 1024 / 1024",
            "legendFormat": "RSS Memory (GB)"
          },
          {
            "expr": "music21_memory_usage_bytes{type=\"vms\"} / 1024 / 1024 / 1024",
            "legendFormat": "Virtual Memory (GB)"
          }
        ],
        "yAxes": [
          {
            "label": "Memory (GB)",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 10,
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "music21_cpu_usage_percent",
            "legendFormat": "CPU Usage %"
          }
        ],
        "yAxes": [
          {
            "label": "CPU %",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 11,
        "title": "Security Events",
        "type": "graph",
        "targets": [
          {
            "expr": "increase(music21_security_violations_total[1h])",
            "legendFormat": "{{ violation_type }}"
          }
        ],
        "yAxes": [
          {
            "label": "Events/hour",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32}
      },
      {
        "id": 12,
        "title": "Authentication Success Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(music21_auth_attempts_total{status=\"success\"}[5m])) / sum(rate(music21_auth_attempts_total[5m])) * 100",
            "legendFormat": "Success Rate %"
          }
        ],
        "yAxes": [
          {
            "label": "Success Rate %",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32}
      },
      {
        "id": 13,
        "title": "Circuit Breaker States",
        "type": "table",
        "targets": [
          {
            "expr": "music21_circuit_breaker_state",
            "legendFormat": "{{ circuit_name }}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 40}
      },
      {
        "id": 14,
        "title": "Bulkhead Utilization",
        "type": "graph",
        "targets": [
          {
            "expr": "music21_bulkhead_active_count / music21_bulkhead_max_concurrent * 100",
            "legendFormat": "{{ bulkhead_name }} Utilization %"
          }
        ],
        "yAxes": [
          {
            "label": "Utilization %",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 40}
      },
      {
        "id": 15,
        "title": "File Processing by Format",
        "type": "pie",
        "targets": [
          {
            "expr": "sum(increase(music21_files_processed_total[24h])) by (format)",
            "legendFormat": "{{ format }}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 48}
      },
      {
        "id": 16,
        "title": "Feature Usage",
        "type": "table",
        "targets": [
          {
            "expr": "sum(increase(music21_feature_usage_total[24h])) by (feature_name)",
            "legendFormat": "{{ feature_name }}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 48}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 36,
    "version": 1,
    "editable": true
  }
}